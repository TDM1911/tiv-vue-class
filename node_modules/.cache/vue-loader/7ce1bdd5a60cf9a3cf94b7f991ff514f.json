utton>\n  </form>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      phone: null,\n      favorite: false,\n      email: null,\n      name: null,\n    };\n  },\n  methods: {\n    createNew() {\n      this.$emit('create-new-friend', {\n        id: Math.random().toString(36).substr(2, 9),\n        name: this.name,\n        phone: this.phone,\n        email: this.email,\n        favorite: this.favorite,\n      });\n    }\n  },\n\n  emits: {\n    'create-new-friend': function (friend) {\n      if (friend.phone && friend.email && friend.name) {\n        return true;\n      }\n      return false;\n    }\n  },\n\n  props: {\n    //\n  },\n};\n</script>\n"]}]}