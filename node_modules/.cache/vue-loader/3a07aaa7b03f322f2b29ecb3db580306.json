etailsAreVisible\">\n      <li>\n        <strong>Phone:</strong>\n        {{ friend.phone }}\n      </li>\n      <li>\n        <strong>Email:</strong>\n        {{ friend.email }}\n      </li>\n    </ul>\n  </li>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      detailsAreVisible: false,\n      friend: {\n        id: this.id,\n        name: this.username,\n        phone: this.phoneNumber,\n        email: this.emailAddress,\n      },\n    };\n  },\n  methods: {\n    toggleDetails() {\n      this.detailsAreVisible = !this.detailsAreVisible;\n    },\n\n    toggleFavorite() {\n      this.$emit('toggle-favorite', this.id);\n    },\n\n    deleteFriend() {\n      this.$emit('delete', this.id)\n    }\n  },\n\n  emits: {\n    'toggle-favorite': function (id) {\n      if (id) {\n        return true;\n      }\n      console.warn('id is missing.');\n      return false;\n    },\n    'delete': function (id) {\n      if (id) {\n        return true;\n      }\n      console.warn('id is missing.');\n      return false;\n    }\n  },\n\n  props: {\n    id: {\n      type: String,\n      required: true,\n    },\n    username: {\n      type: String,\n      required: true,\n    },\n    emailAddress: {\n      type: String,\n      required: true,\n    },\n    phoneNumber: {\n      type: [String, Number],\n      required: false,\n      default: 'N/A',\n    },\n    isFavorite: {\n      type: Boolean,\n      required: false,\n      default: false,\n    },\n  },\n};\n</script>\n"]}]}