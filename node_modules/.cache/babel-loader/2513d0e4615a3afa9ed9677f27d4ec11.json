    email: this.emailAddress,\n      },\n    };\n  },\n  methods: {\n    toggleDetails() {\n      this.detailsAreVisible = !this.detailsAreVisible;\n    },\n\n    toggleFavorite() {\n      this.$emit('toggle-favorite', this.id);\n    },\n\n    deleteFriend() {\n      this.$emit('delete', this.id)\n    }\n  },\n\n  emits: {\n    'toggle-favorite': function (id) {\n      if (id) {\n        return true;\n      }\n      console.warn('id is missing.');\n      return false;\n    },\n    'delete': function (id) {\n      if (id) {\n        return true;\n      }\n      console.warn('id is missing.');\n      return false;\n    }\n  },\n\n  props: {\n    id: {\n      type: String,\n      required: true,\n    },\n    username: {\n      type: String,\n      required: true,\n    },\n    emailAddress: {\n      type: String,\n      required: true,\n    },\n    phoneNumber: {\n      type: [String, Number],\n      required: false,\n      default: 'N/A',\n    },\n    isFavorite: {\n      type: Boolean,\n      required: false,\n      default: false,\n    },\n  },\n};\n</script>\n"]}]}